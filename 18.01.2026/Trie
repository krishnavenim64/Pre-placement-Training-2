class Trie {
    class Node {
        Node[] next = new Node[26];
        boolean end;
    }
    Node root = new Node();

    public void insert(String word) {
        Node cur = root;
        for (char c : word.toCharArray()) {
            if (cur.next[c-'a'] == null)
                cur.next[c-'a'] = new Node();
            cur = cur.next[c-'a'];
        }
        cur.end = true;
    }
}
